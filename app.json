{
  "expo": { 
    "name": "typescript-app",
    "slug": "typescript-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./src/App.tsx",
    "icon": "./src/presentation/assets/icons/icon.png",
    "splash": {
      "image": "./src/presentation/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./src/presentation/assets/favicon.png"
    }
  }
}
